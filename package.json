{
  "name": "lq-component",
  "version": "1.0.1",
  "description": "",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6.5.0",
    "npm": ">=5.0.0"
  },
  "config": {
    "port": 4000
  },
  "scripts": {
    "clean": "rimraf build",
    "clean:map": "rimraf build/*.map",
    "compile": "cross-env DEBUG=app:* node bin/compile && npm run clean:map",
    "compile:dev": "cross-env NODE_ENV=develop npm run compile",
    "compile:qafc": "cross-env NODE_ENV=release npm run compile",
    "compile:pre": "cross-env NODE_ENV=uat npm run compile",
    "compile:online": "cross-env NODE_ENV=production npm run compile",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "cross-env PORT=$npm_package_config_port DEBUG=app:* NODE_ENV=local node bin/dev-server",
    "start:mock": "cross-env MOCK=true npm start",
    "dev": "cross-env DEBUG=app:* NODE_ENV=local nodemon bin/dev-server --ignore build --ignore coverage --ignore tests --ignore src",
    "test": "cross-env DEBUG=app:* NODE_ENV=test node ./node_modules/karma/bin/karma start config/karma.config",
    "test:dev": "npm run test -- --watch",
    "build": "cross-env DEBUG=app:* npm run lint && npm run test && npm run clean && npm run compile",
    "build:dev": "cross-env NODE_ENV=develop npm run build",
    "build:qafc": "cross-env NODE_ENV=release npm run build",
    "build:online": "cross-env NODE_ENV=production npm run build",
    "git:commit": "rimraf dist && babel src/components -d dist --copy-files && git add . && git commit -m",
    "prepublishOnly": "npm version patch && git push",
    "babel": "rimraf dist && babel src/components -d dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luoqi14/lq-component.git"
  },
  "author": "luoq@xinguangnet.com",
  "license": "MIT",
